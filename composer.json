{
    "name": "spatie/calendar",
    "description": "A simple calendar",
    "keywords": [
        "spatie",
        "Calendar"
    ],
    "homepage": "https://github.com/spatie/Calendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruben Van Assche",
            "email": "ruben@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-mbstring" : "*"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Calendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Calendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Calendar\\CalendarServiceProvider"
            ]
        }
    }
}
